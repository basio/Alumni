<h1>Listing alumnus</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Degree</th>
    <th>Year</th>
    <th>Email</th>
    <th>Address</th>
    <th>Bio</th>
    <th>Resume</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alumnus.each do |alumnu| %>
  <tr>
    <td><%= alumnu.id %></td>
    <td><%= alumnu.name %></td>
    <td><%= alumnu.degree %></td>
    <td><%= alumnu.year %></td>
    <td><%= alumnu.email %></td>
    <td><%= alumnu.address %></td>
    <td><%= alumnu.bio %></td>
    <td><%= alumnu.resume %></td>
    <td><%= link_to 'Show', alumnu %></td>
    <td><%= link_to 'Edit', edit_alumnu_path(alumnu) %></td>
    <td><%= link_to 'Destroy', alumnu, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Alumnu', new_alumnu_path %>
